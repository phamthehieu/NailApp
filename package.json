{
  "name": "NailApp",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "start:dev": "cross-env NODE_ENV=development react-native start",
    "start:prod": "cross-env NODE_ENV=production react-native start",
    "build:android:dev": "cross-env NODE_ENV=development react-native run-android --variant=debug",
    "build:android:prod": "cross-env NODE_ENV=production react-native run-android --variant=release",
    "build:ios:dev": "cross-env NODE_ENV=development react-native run-ios --configuration=Debug",
    "build:ios:prod": "cross-env NODE_ENV=production react-native run-ios --configuration=Release",
    "bundle:android:dev": "cross-env NODE_ENV=development react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:android:prod": "cross-env NODE_ENV=production react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios:dev": "cross-env NODE_ENV=development react-native bundle --platform ios --dev true --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/",
    "bundle:ios:prod": "cross-env NODE_ENV=production react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/",
    "update-version": "node scripts/update-version.js"
  },
  "dependencies": {
    "@legendapp/motion": "^2.3.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/drawer": "^7.7.2",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "apisauce": "^3.1.1",
    "date-fns": "^4.1.0",
    "i18next": "25.4.2",
    "intl-pluralrules": "^2.0.1",
    "lottie-react-native": "^7.2.2",
    "lucide-react-native": "^0.544.0",
    "react": "19.0.0",
    "react-aria": "^3.33.0",
    "react-dom": "19.0.0",
    "react-i18next": "15.7.2",
    "react-native": "0.79.5",
    "react-native-date-picker": "^5.0.13",
    "react-native-dotenv": "^3.4.11",
    "react-native-drawer-layout": "^4.0.1",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-flexbox-grid": "^0.3.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.12.7",
    "react-native-keychain": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.5.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.13.0",
    "react-native-web": "^0.20.0",
    "react-native-worklets": "^0.5.1",
    "react-stately": "^3.39.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.5",
    "@react-native/eslint-config": "0.79.5",
    "@react-native/metro-config": "0.79.5",
    "@react-native/typescript-config": "0.79.5",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
